{
    "name": "@babylonjs/shared-ui-components",
    "version": "6.40.0",
    "main": "index.js",
    "module": "index.js",
    "types": "index.d.ts",
    "files": [
        "**/*.js",
        "**/*.d.ts",
        "**/*.map",
        "**/*.svg",
        "**/*.scss",
        "readme.md",
        "license.md"
    ],
    "scripts": {
        "build": "npm run clean && npm run compile",
        "clean": "rimraf dist && rimraf *.tsbuildinfo && rimraf \"./**/*.!(md|json|build.json)\"",
        "compile": "tsc -b tsconfig.build.json --verbose",
        "precompile": "build-tools -c process-assets --path-prefix ../../../dev/sharedUiComponents/ --output-dir ./",
        "postcompile": "build-tools -c add-js-to-es6"
    },
    "devDependencies": {
        "@dev/build-tools": "^1.0.0",
        "@lts/core": "1.0.0",
        "@lts/gui": "1.0.0"
    },
    "peerDependencies": {
        "@types/dagre": "^0.7.47",
        "dagre": "^0.8.5",
        "react": "^17.0.2",
        "react-dnd": "15.0.1",
        "react-dnd-touch-backend": "15.0.1",
        "react-dom": "^17.0.2"
    },
    "keywords": [
        "3D",
        "javascript",
        "html5",
        "webgl",
        "babylon.js"
    ],
    "license": "Apache-2.0",
    "esnext": "index.js",
    "type": "module",
    "sideEffects": true,
    "homepage": "https://www.babylonjs.com",
    "repository": {
        "type": "git",
        "url": "https://github.com/BabylonJS/Babylon.js.git"
    },
    "bugs": {
        "url": "https://github.com/BabylonJS/Babylon.js/issues"
    }
}
