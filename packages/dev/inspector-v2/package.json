        "@pmmmwh/react-refresh-webpack-plugin": "^0.6.0",
        "babel-loader": "^10.0.0",
        "html-webpack-plugin": "^5.5.0",
        "react-refresh": "^0.17.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "usehooks-ts": "^3.1.1",
        "webpack": "^5.64.0",
