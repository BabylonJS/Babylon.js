{
    "name": "@tools/babylon-server",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "clean": "rimraf dist && rimraf declarations",
        "static-serve": "http-server ",
        "serve": "webpack serve",
        "serve:prod": "npm run serve:source:prod",
        "serve:source": "webpack serve",
        "serve:lts": "npm run build:declaration:lts && webpack serve --env=source=lts",
        "serve:source:prod": "webpack serve --env=mode=production",
        "build": "npm run build:dev",
        "build:dev": "npm run build:source:dev && npm run build:declaration",
        "build:prod": "npm run build:source:dev -- --env=mode=production && npm run build:declaration",
        "build:snapshot": "npm run clean && npm run build:declaration:lts && webpack --config webpack.config.snapshot.js --env=mode=production",
        "build:source:dev": "webpack",
        "build:source:lts": "webpack --env=source=lts",
        "build:declaration": "build-tools -c pud --config ./declarationConfigDev.json",
        "build:declaration:lts": "build-tools -c pud --config ./declarationConfigLTS.json",
        "watch:declaration:dev": "build-tools -c pud --config ./declarationConfigDev.json --watch",
        "watch:declaration:lts": "build-tools -c pud --config ./declarationConfigLTS.json --watch",
        "prepare": "node ./scripts/generateScripts.js"
    },
    "devDependencies": {
        "@dev/build-tools": "1.0.0",
        "@dev/core": "1.0.0",
        "@dev/gui": "1.0.0",
        "@dev/inspector": "1.0.0",
        "@dev/loaders": "1.0.0",
        "@dev/post-processes": "1.0.0",
        "@dev/procedural-textures": "1.0.0",
        "@dev/serializers": "1.0.0",
        "@dev/shared-ui-components": "1.0.0",
        "@tools/node-editor": "1.0.0",
        "@tools/gui-editor": "1.0.0",
        "@tools/ktx2decoder": "1.0.0",
        "@lts/core": "1.0.0",
        "@lts/gui": "1.0.0",
        "@lts/loaders": "1.0.0",
        "@lts/post-processes": "1.0.0",
        "@lts/procedural-textures": "1.0.0",
        "@lts/serializers": "1.0.0",
        "mini-css-extract-plugin": "^2.5.3",
        "source-map-loader": "^4.0.0",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.2.6",
        "typescript": "^4.4.4",
        "webpack": "^5.73.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.7.3",
        "webpack-merge": "^5.8.0"
    },
    "sideEffects": true
}
